{
	"dev": true,
	"compilers": {
		"ignore": [
			"layout.pug",
			"navbar.pug",
			"reference.pug",
			"contact-form.pug",
			"contact.pug",
			"footer.pug"
		],
		"javascript": {
			"minified": true,
			"minifiedOnly": false
		},
		"scss": {
			"minified": true,
			"minifiedOnly": true
		},
		"pug": {
			"minified": true
		},
		"image": {
			"default": "webp",
			"rules": [],
			"styles": {
				"webp": {
					"webp": {},
					"modulate": {
						"brightness": 1,
						"saturation": 1,
						"hue": 1,
						"lightness": 1
					}
				}
			}
		}
	},
	"seo": {
		"host": "",
		"sitemap": {
			"auto": true,
			"frequency": "daily",
			"extensions": [
				"html"
			],
			"ignore": []
		},
		"robots": {
			"auto": true,
			"content": [
				"User-agent: Googlebot",
				"Disallow: /nogooglebot/",
				"",
				"User-agent: *",
				"Allow: /"
			]
		},
		"google": {
			"gtm": {
				"token": ""
			},
			"ga": {
				"token": ""
			}
		}	
	}
}